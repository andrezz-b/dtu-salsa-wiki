{
  "name": "dtu-salsa-wiki",
  "version": "5.8.2",
  "description": "DTU Salsa Wiki",
  "author": "andrezz-b",
  "type": "module",
  "scripts": {
    "dev": "npm run import-obsidian && npm run generate-json && astro dev",
    "build": "npm run import-obsidian && npm run generate-json && astro build",
    "preview": "astro preview",
    "check": "astro check",
    "format": "prettier -w ./src",
    "clean": "rm -rf node_modules package-lock.json .astro .json dist",
    "generate-json": "node scripts/jsonGenerator.js",
    "remove-darkmode": "node scripts/removeDarkmode.js && npm run format",
    "import-obsidian": "npx tsx scripts/import-obsidian.ts --moves-out src/content/moves --concepts-out src/content/concepts --obsidian-data obsidian-data"
  },
  "dependencies": {
    "@astro-community/astro-embed-youtube": "^0.5.10",
    "@astrojs/check": "^0.9.2",
    "@astrojs/mdx": "^4.3.13",
    "@astrojs/react": "^4.4.2",
    "@astrojs/sitemap": "^3.6.1",
    "@digi4care/astro-google-tagmanager": "^1.6.0",
    "@vite-pwa/astro": "^1.2.0",
    "astro": "^5.16.7",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "marked": "^17.0.0",
    "minisearch": "^7.2.0",
    "prop-types": "^15.8.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^9.0.0",
    "sharp": "^0.34.5",
    "vite": "^7.2.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.1",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3"
  }
}
